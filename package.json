{
  "name": "probos",
  "version": "0.0.1",
  "description": "Takes the size/gzip and git info of the packages and submits them to probos",
  "main": "index.js",
  "scripts": {
    "fix": "prettier --ignore-path=.gitignore --write '**/*.js'",
    "lint": "prettier --ignore-path=.gitignore --check '**/*.js'",
    "jest": "jest",
    "test": "npm run lint && npm run jest"
  },
  "bin": {
    "gzip-size": "cli.js"
  },
  "keywords": [
    "probos"
  ],
  "engines": {
    "node": ">=6"
  },
  "author": "Oleg Gaidarenko <markelog@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^3.0.0",
    "gzip-size": "^5.1.1",
    "isomorphic-unfetch": "^3.0.0",
    "last-commit-log": "^2.1.0",
    "meow": "^6.0.0"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "prettier": "^1.19.1"
  }
}
